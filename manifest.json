{
  "manifest_version": 2,

  "name": "InstaGIF",
  "description": "Easily create Gifs",
  "version": "1.3",

  "browser_action": {
    "default_icon": "peanut.png"
  },
  "icons": { "16": "peanut.png",
           "48": "peanut.png",
          "128": "peanut.png" },
  "background": {
    "page": "popup.html"
  },
  "permissions": [
    "tabs"
  ]
}
